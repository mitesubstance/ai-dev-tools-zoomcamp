{
  "name": "coding-interview-platform",
  "version": "0.2.0",
  "description": "Real-time collaborative coding interview platform",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install"
  },
  "keywords": [
    "coding-interview",
    "collaborative",
    "real-time",
    "fastapi",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
